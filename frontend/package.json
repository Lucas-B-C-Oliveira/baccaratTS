{
  "name": "baccarat",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "./public/electron.js",
  "homepage": "/",
  "productName": "Baccarat",
  "author": {
    "name": "Lucas B C Oliveira",
    "email": "lucascoliveira819@gmail.com"
  },
  "description": "teste",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lintFix": "eslint src --ext .ts,.tsx --fix",
    "electron": "electron .",
    "esbuild-cjs": "esbuild ./public/electron.js --format=cjs --outfile=./build/electron.js",
    "electron:dev": "npm run esbuild-cjs && electron .",
    "electron:build": "npm run esbuild-cjs && electron-packager --out=build .",
    "build:all": "npm run build && npm run electron:build",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js",
    "react:build:electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "compression": "normal",
    "removePackageScripts": true,
    "appId": "com.electron.baccarat",
    "productName": "Baccarat",
    "directories": {
      "buildResources": "public"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      "build/server"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.10.1",
    "react-router-dom": "^6.3.0",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^2.0.0",
    "electron": "^19.0.9",
    "electron-builder": "^23.3.3",
    "electron-packager": "^15.5.1",
    "esbuild": "^0.14.51",
    "eslint": "^8.19.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
